<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activityCaracteristiquesEmployeActivity">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="165dp"
        app:srcCompat="@drawable/logoakei" />


    <TextView
        android:id="@+id/et_titreEmploye"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Nom de l'employe"
        android:textSize="20sp" />


    <TextView
        android:id="@+id/et_nomEmployee"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="et_nomEmploye"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/et_prenomEmploye"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="et_prenomEmploye"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/et_idEmploye"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="et_idEmploye"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/et_rayon"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="et_rayon"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/et_magasin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="et_magasin"
        android:textSize="20sp" />



</LinearLayout>