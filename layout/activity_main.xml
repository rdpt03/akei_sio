<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="165dp"
        app:srcCompat="@drawable/logoakei" />

    <Button
        android:id="@+id/buttonProduit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Voir un Produit" />

    <Button
        android:id="@+id/buttonRecherche"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Voir les vehicules à louer" />

    <Button
        android:id="@+id/bt_listeRayonEmploye"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Voir les affectations employés" />


</LinearLayout>